    "@eslint/js": "^9.36.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.19",
    "@types/ws": "^8.5.10",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "ts-node": "^10.9.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
